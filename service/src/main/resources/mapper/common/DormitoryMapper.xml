<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lushihao.service.dao.DormitoryMapper">
    <select id="selectAll" resultType="com.lushihao.service.common.Dormitory">
        select * from
            `dormitory`
    </select>
    <select id="selectOne" resultType="com.lushihao.service.common.Dormitory">
        select * from
            `dormitory`
        where
            `id`=#{id}
    </select>
</mapper>